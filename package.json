{
  "name": "@jit017/realtimepr",
  "version": "1.1.0",
  "description": "A CLI tool for real-time code review and feedback",
  "type": "module",
  "main": "dist/cli.js",
  "bin": {
    "realtimepr": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepare": "npm run build"
  },
  "keywords": [
    "code-review",
    "linting",
    "best-practices",
    "code-analysis",
    "cli"
  ],
  "author": "Jit017",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jit017/realtimepr.git"
  },
  "bugs": {
    "url": "https://github.com/Jit017/realtimepr/issues"
  },
  "homepage": "https://realtime-pr.vercel.app/",
  "dependencies": {
    "@babel/eslint-parser": "^7.23.10",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "openai": "^4.28.0",
    "tmp": "^0.2.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@types/tmp": "^0.2.6",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
